{
  "name": "bampouris-eu",
  "version": "1.0.0",
  "private": true,
  "description": "Personal web page/blog with Docusaurus",
  "author": {
    "name": "Theoklitos Bampouris",
    "email": "th.bampouris@gmail.com",
    "url": "https://www.bampouris.eu/"
  },
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc",
    "prepare": "husky install",
    "precommit": "pretty-quick --staged && [ $? -eq 0 ] && typecheck && [ $? -eq 0 ] && lint-staged",
    "commitmsg": "commitlint --edit $1"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.14",
    "@docusaurus/preset-classic": "2.0.0-beta.14",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.2.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.48.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.14",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^17.0.38",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^5.1.3",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.5.2"
  }
}
